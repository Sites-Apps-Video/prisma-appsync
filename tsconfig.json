{
    "compilerOptions": {
        "baseUrl": ".",
        "types": ["node", "lodash"],
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "noUnusedLocals": true,
        "allowJs": false,
        "strictNullChecks": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": false
    },
    "exclude": ["dist", "node_modules"]
}
